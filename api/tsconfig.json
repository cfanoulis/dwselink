{
	"compilerOptions": {
		"noEmit": true,
		"allowJs": true,
		"esModuleInterop": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"baseUrl": "./",
		"rootDirs": ["./src", "../.redwood/types/mirror/api/src"],
		"paths": {
			"src/*": ["./src/*", "../.redwood/types/mirror/api/src/*"],
			"types/*": ["./types/*"]
		}
	},
	"include": ["src", "../.redwood/types/includes/all-*", "../.redwood/types/includes/api-*"]
}
